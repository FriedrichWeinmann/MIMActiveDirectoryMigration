        <sourcename>UserPrincipalName</sourcename>
        <name>UserPrincipalName</name>
        <type>replace</type>
        <value>@fabrikam.org$</value>
        <newvalue>@contoso.com</newvalue>
      </attribute>
	  -->
    </attributes>
  </import>
